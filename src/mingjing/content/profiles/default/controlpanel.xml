<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="mingjing.content">

    <configlet
        title="MJNews setup"
        action_id="mingjing.content.mjnetSetting"
        appId="mingjing.content"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@mjnetSettingControlPanel"
        icon_expr="string:/favicon.ico"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
